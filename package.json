    "@types/passport-jwt": "^3.0.9",
    "@types/socket.io": "^3.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "firebase": "^11.0.1",
    "firebase-admin": "^12.3.1",
    "generate-password": "^1.7.1",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "mysql2": "^3.6.0",
    "nestjs-uuid": "^0.1.4",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.3",
